<p id="notice"><%= notice %></p>

<h1>Afterhourmods</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @afterhourmods.each do |afterhourmod| %>
      <tr>
        <td><%= content_tag "a", 'Show', href: url_for(afterhourmod) %></td>
        <td><%= content_tag "a", 'Edit', href:edit_afterhourmod_path(afterhourmod) %></td>
        <td><%= content_tag "a", 'X', href: url_for(afterhourmod), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= content_tag "a", 'New Afterhourmod', href:new_afterhourmod_path %>
